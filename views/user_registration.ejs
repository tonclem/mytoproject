<%- include('header'); -%>
<%- include('navbar'); -%>



<div class="container">


    <div class="page-header">

        <h1 style="font:22px Arial;margin: 20px;color: darkgreen">
           User registration</h1>

    </div>


    <div class="jumbotron">
        <h3>
            Register Now</h3>
        <form action="/user_registration" method="POST" enctype="multipart/form-data">
        
            <input type="text" name="username" placeholder="Enter name"> <br/>
            <input type="number" name="phone" placeholder="Enter phone Number"> <br/>
            <input type="text" name="password" placeholder="Enter password"> <br/>
            <input type="file" name="image" placeholder="Pick your image"> <br/>
            <input class="btn btn-outline-success my-2 my-sm-0" type ="submit" name = "btnSubmit" value = "Register"/>
        </form>
    </div>

</div>





<%- include('footer'); -%>